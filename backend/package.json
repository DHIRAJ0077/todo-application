{
  "name": "todo-backend",
  "version": "1.0.0",
  "description": "Todo app backend with Express, Mongoose, and MongoDB",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch server.js",
    "prestart": "prisma generate --schema=./prisma/schema.prisma",
    "start": "node server.js",
    "build": "prisma generate --schema=./prisma/schema.prisma",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mongoose": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "prisma": "^7.3.0"
  }
}
